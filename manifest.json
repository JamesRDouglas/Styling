{
	"name": "Styling",
	"short_name": "Styling",
	"author": "James Douglas",
	"description": "Styling is a simple add-on which lets you add css styles to any webpage.",
	"manifest_version": 2,
	"version": "0.4.9.3",
	"icons": { "128": "Styling.png" },
	"options_ui": { "page": "src/options.html" },
	"applications": { "gecko": { "id": "{4acf1e87-8461-4606-a7c4-d4890aa2147a}" } },
	"permissions": [
		"tabs",
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"contextMenus",
		"storage",
		"activeTab",
		"<all_urls>"
	],
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"run_at": "document_start",
			"all_frames": true,
			"match_about_blank": true,
			"js": [ "src/inject.js" ]
		}
	],
	"web_accessible_resources": [ "Styling.png", "src/inject.js", "src/options.js", "src/options.html", "src/manage.js", "src/manage.html" ],
	"browser_action": {
		"default_icon": { "128": "Styling.png" },
		"default_title": "Styling",
		"default_popup": "src/options.html"
	}
}